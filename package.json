{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:migrate": "node -e \"console.log('Run SQL scripts in /scripts folder via Supabase dashboard or CLI')\"",
    "db:seed": "node -e \"console.log('Run seed scripts in /scripts folder')\"",
    "deploy:functions": "supabase functions deploy reset-referral-counter",
    "dev": "next dev",
    "keys:backup": "bash ./scripts/backup-keys.sh",
    "keys:rotate": "bash ./scripts/rotate-keys.sh",
    "keys:verify": "bash ./scripts/verify-keys.sh",
    "lh:local": "lighthouse http://localhost:3000 --quiet --only-categories=performance,accessibility,best-practices,seo --output=json --output-path=./lighthouse-local.json",
    "lh:prod": "lighthouse https://v0-weekchainmvp.vercel.app/ --quiet --only-categories=performance,accessibility,best-practices,seo --output=json --output-path=./lighthouse-report.json",
    "lh:prod:html": "lighthouse https://v0-weekchainmvp.vercel.app/ --only-categories=performance,accessibility,best-practices,seo --output=html --output-path=./lighthouse-report.html",
    "lint": "next lint",
    "precommit": "npm run lint && npm run validate-env",
    "predeploy": "npm run lint && npm run build && npm run validate-env",
    "security:check": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "start": "next start",
    "test:a11y": "npm run lh:prod && node -e \"const report = require('./lighthouse-report.json'); const a11y = report.categories.accessibility.score * 100; console.log('Accessibility Score:', a11y); process.exit(a11y >= 95 ? 0 : 1);\"",
    "test:all": "npm run test:a11y && npm run test:perf",
    "test:compliance": "bash ./scripts/test-compliance.sh",
    "test:fraud": "node scripts/test-fraud-detection.js",
    "test:perf": "npm run lh:prod && node -e \"const report = require('./lighthouse-report.json'); const perf = report.categories.performance.score * 100; console.log('Performance Score:', perf); process.exit(perf >= 90 ? 0 : 1);\"",
    "validate-env": "node -e \"require('./lib/config/environment').validateEnvironment()\""
  },
